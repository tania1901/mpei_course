//https://leetcode.com/problems/fizz-buzz/

class Solution {
public:
    vector<string> fizzBuzz(int n) {
        vector <string> helper;
        for(int i=1; i<=n; i++){
            if(i%15==0) helper.push_back("FizzBuzz");
                else if(i%5==0)helper.push_back("Buzz");
                    else if(i%3==0) helper.push_back("Fizz");
                        else helper.push_back(to_string(i));
        }
        return helper;
    }
};
