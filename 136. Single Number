//https://leetcode.com/problems/single-number/

class Solution {
public:
     int singleNumber(vector<int>& nums) {
        int n = nums.size();
        int x = 0;
        for(int i=0; i<n; i++){
            x = x^nums[i];//получается запоминание только 1 числа
            cout<<x;
        }
        return x;
    }
};
