//https://leetcode.com/problems/rotate-array/


class Solution {
public:
    
       void rotate(vector<int>& nums, int k) {
        if(k<=0) return;   
           if(nums.size()==0) return;
       for(int i=0; i<k; i++){
           int ch=nums[0];
           nums.erase(nums.begin());
           nums.push_back(ch);
           
       }
    }
};
